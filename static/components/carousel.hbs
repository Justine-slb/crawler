    {{#if_eq current "10"}}
        {{> carouselItem current=true parameter=parameter position=10 element=this.[9]}}
    {{else}}
        {{> carouselItem current=false parameter=parameter position=10 element=this.[9]}}
    {{/if_eq}}
</div>