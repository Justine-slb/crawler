<div class="carousel">
    {{#if_eq current "1"}}
        {{> carouselItem current=true parameter=parameter position=1 element=this.[0]}}
    {{else}}
        {{> carouselItem current=false parameter=parameter position=1 element=this.[0]}}
    {{/if_eq}}

    {{#if_eq current "2"}}
        {{> carouselItem current=true parameter=parameter position=2 element=this.[1]}}
    {{else}}
        {{> carouselItem current=false parameter=parameter position=2 element=this.[1]}}
    {{/if_eq}}

    {{#if_eq current "3"}}
        {{> carouselItem current=true parameter=parameter position=3 element=this.[2]}}
    {{else}}
